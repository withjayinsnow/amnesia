}

.box_icon image {
	/* padding-top: 30rpx; */
	/* vertical-align: middle !important; */
	width: 24rpx;
	height: 24rpx;
	margin-right: 12px;

}

.loginMain {
	background-color: #fff;
	height: 100vh;
}

.login {
	display: flex;
	justify-content: center;
}

.loginButton {
	margin-top: 200rpx;
	color: white;
}

.loginDesc {
	margin-top: 20rpx;
	/* border: 1px red solid; */
	color: gray;
	text-align: center;
}

.edition {
	width: 100%;
	position: absolute;
	bottom: 12px;
	color: gray;
	text-align: center;
	opacity:0.6;
}
